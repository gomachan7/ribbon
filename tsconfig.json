{
  "compilerOptions": {
    "baseUrl": "src/scripts",
    "paths": {
      "phaser/*": ["../../node_modules/phaser/src/*"]
    },
    "sourceMap": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "module": "commonjs",
    "target": "es5"
  },
  "include": ["./src/scripts/**/*", "./node_modules/phaser/src/**/*"]
}
